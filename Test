Tabs.hahaha:AddButton({
        Title = "Teleport To Random",
        Description = "Button",
        Callback = function()
local Players = game:GetService("Players")

local function teleportToRandomPlayer()
    local allPlayers = Players:GetPlayers()
    local localPlayer = Players.LocalPlayer

    for i, player in ipairs(allPlayers) do
        if player == localPlayer then
            table.remove(allPlayers, i)
            break
        end
    end

    if #allPlayers > 0 then
        local randomPlayer = allPlayers[math.random(1, #allPlayers)]
        local character = localPlayer.Character
        local randomCharacter = randomPlayer.Character

        if character and randomCharacter then
            local randomHumanoidRootPart = randomCharacter:FindFirstChild("HumanoidRootPart")
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")

            if randomHumanoidRootPart and humanoidRootPart then
                humanoidRootPart.CFrame = randomHumanoidRootPart.CFrame
            end
        end
    else
        warn("No other players to teleport to.")
    end
end

teleportToRandomPlayer()

local particleEffect = game.ReplicatedStorage.Resources.FiveSeasonsFX.CharFX.TP
local clonedEffect = particleEffect:Clone()
clonedEffect.Parent = game.Players.LocalPlayer.Character["HumanoidRootPart"]
for _, child in ipairs(clonedEffect:GetChildren()) do
    if child:IsA("ParticleEmitter") then
        child:Emit(5)
    end
end

local soundeffect = Instance.new("Sound")
soundeffect.SoundId = "rbxassetid://4861638982"
soundeffect.Parent = game.Players.LocalPlayer.Character.Torso
soundeffect:Play()
soundeffect.Volume = 5

