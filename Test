Tabs.hahahah1:AddButton({
        Title = "Unlimited Flex Works",
        Description = "Button",
        Callback = function()

local SkillName = "Unlimited Flex Works" --[ skills cant have the same name or it will bug, ]
local CooldownLengh = 5
local Burning = true -- [ the KJ UFW burning effect ]

local Activated = function()
--code here
task.spawn(function()
local function removeAndReturnTools(player)
    -- Get the player's tools
    local backpack = player.Backpack
    local tools = {}
    
    -- Store tools in the workspace and remove them from the backpack
    for _, tool in ipairs(backpack:GetChildren()) do
        if tool:IsA("Tool") then
            table.insert(tools, tool)
            tool.Parent = game.Workspace
        end
    end

    -- Wait for 25 seconds
    wait(25)

    -- Give tools back to the player's backpack
    for _, tool in ipairs(tools) do
        tool.Parent = backpack
    end
end

-- Call the function for the player when needed (e.g., when the player clicks a button or during an event)
game.Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function()
        removeAndReturnTools(player)
    end)
end)
end)


local function GetGitSound(GithubSnd, SoundName)
    local url = GithubSnd
    if not isfile(SoundName .. ".mp3") then
        writefile(SoundName .. ".mp3", game:HttpGet(url))
    end
    local sound = Instance.new("Sound")
    sound.SoundId = (getcustomasset or getsynasset)(SoundName .. ".mp3")
    sound.Volume = 1  -- Set volume to 1
    sound.Name = SoundName  -- Name the sound
    sound.Parent = workspace
    return sound
end

