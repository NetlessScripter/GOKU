Tabs.Miscellaneous:AddButton({
        Title = "Dive!!  A-One (The Strongest Battlegrounds KJ's Official Theme Song)",
        Description = "Button",
        Callback = function()
task.spawn(function()
local function GetGitSound(GithubSnd, SoundName)
    local url = GithubSnd  
    if not isfile(SoundName..".mp3") then  
        writefile(SoundName..".mp3", game:HttpGet(url))
    end  
    local sound = Instance.new("Sound")
    sound.SoundId = (getcustomasset or getsynasset)(SoundName..".mp3")
    return sound  
end  

local Clock = GetGitSound("https://github.com/VonexuryRemaker/kj-sounds/blob/main/Dive!!%20%20A-One%20(The%20Strongest%20Battlegrounds%20KJ's%20Official%20Theme%20Song)%20%5B%20ezmp3.cc%20%5D.mp3?raw=true", "Dive!!")

Clock.Parent = workspace  
Clock.Volume = 1  
Clock.TimePosition = 0

Clock:Play()
end)
        end
    })

